{
  "name": "jsloops",
  "version": "0.1.0",
  "authors": [
    "Alex Milanov <alex@i4web.biz>"
  ],
  "description": "Music Making App",
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "src/lib",
    "test",
    "tests"
  ],
  "dependencies": {
    "audiosynth": "keithwhor/audiosynth",
    "file-saver.js": "~1.20150507.2",
    "font-awesome": "~4.4.0",
    "jasmid": "*",
    "jquery": "~2.1.4",
    "jquery-ui": "~1.11.4",
    "lodash": "~3.10.1",
    "moment": "~2.10.6",
    "qwerty-hancock": "~0.5.0"
  },
  "overrides": {
    "audiosynth": {
      "main": "audiosynth.js"
    },
    "font-awesome": {
      "main": [
        "css/**/*.css",
        "fonts/**/*"
      ]
    },
    "jasmid": {
      "main": [
        "*.js",
        "*.mid"
      ]
    }
  }
}
